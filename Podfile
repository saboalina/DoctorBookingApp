def shared_pods

  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'
  pod 'Firebase/Messaging'
  
end

target 'DoctorAppointmentBookingApp' do

  shared_pods

end

target 'DoctorAppointmentBookingAppTests' do

  shared_pods
  
end
